	  <li Class="StockGenerator_SingleDef">
        <thingDef>RawMagicyte</thingDef>
		<price>Expensive</price>
        <countRange>
          <min>100</min>
          <max>300</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>TM_Grimoire</thingDef>
        <countRange>-1~1</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>TM_CombatManual</thingDef>
        <countRange>-1~1</countRange>
      </li>
	</value>
  </Operation>
  
   <Operation Class="PatchOperationAdd">
    <xpath>*/TraderKindDef[defName = "Visitor_Outlander_Standard" or defName = "Visitor_Neolithic_Standard"]/stockGenerators</xpath>
	<value>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>RawMagicyte</thingDef>
        <countRange>
          <min>50</min>
          <max>100</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>TM_Grimoire</thingDef>
        <countRange>-4~1</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>TM_CombatManual</thingDef>
