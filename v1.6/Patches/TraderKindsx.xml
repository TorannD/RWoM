<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <!-- Random social interactions -->

  <Operation Class="PatchOperationAdd">
    <xpath>*/TraderKindDef[defName = "Base_Neolithic_Standard" or defName = "Base_Outlander_Standard" or defName = "Caravan_Outlander_Exotic" or defName = "Caravan_Neolithic_ShamanMerchant" or defName = "Orbital_Exotic"]/stockGenerators</xpath>
	<value>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>RawMagicyte</thingDef>
		<price>Expensive</price>
        <countRange>
          <min>400</min>
          <max>700</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>RawMagicyte</thingDef>
		<price>Expensive</price>
        <countRange>
          <min>100</min>
          <max>300</max>
        </countRange>
      </li>
	  <li Class="StockGenerator_SingleDef">
        <thingDef>TM_Grimoire</thingDef>
        <countRange>0~2</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>TM_CombatManual</thingDef>
        <countRange>0~2</countRange>
      </li>
	</value>
  </Operation>
  
  <Operation Class="PatchOperationAdd">
    <xpath>*/TraderKindDef[defName = "Caravan_Outlander_BulkGoods" or defName = "Caravan_Outlander_PirateMerchant" or defName = "Caravan_Neolithic_BulkGoods" or defName = "Caravan_Outlander_CombatSupplier"]/stockGenerators</xpath>
