          </li>
		  <li>
            <capacity>Breathing</capacity>
            <offset>0.20</offset>
          </li>
		  <li>
            <capacity>BloodPumping</capacity>
            <offset>0.20</offset>
          </li>
		  <li>
            <capacity>BloodFiltration</capacity>
            <offset>0.20</offset>
          </li>
        </capMods>
		<statOffsets>
			<AimingDelayFactor>-0.10</AimingDelayFactor>
			<PainShockThreshold>.24</PainShockThreshold>
			<ArmorRating_Blunt>.3</ArmorRating_Blunt>
			  <ArmorRating_Sharp>.3</ArmorRating_Sharp>
			  <ArmorRating_Heat>.3</ArmorRating_Heat>
			  <ArmorRating_Alignment>.3</ArmorRating_Alignment>
		</statOffsets>
      </li>
	  <li>
        <label>III</label>
        <minSeverity>2</minSeverity>
		<becomeVisible>true</becomeVisible>
		<capMods>
		  <li>
            <capacity>Moving</capacity>
            <offset>0.55</offset>
		  </li>
	      <li>
            <capacity>Manipulation</capacity>
            <offset>0.55</offset>
          </li>
		  <li>
            <capacity>Breathing</capacity>
            <offset>0.30</offset>
          </li>
		  <li>
            <capacity>BloodPumping</capacity>
            <offset>0.30</offset>
          </li>
		  <li>
            <capacity>BloodFiltration</capacity>
            <offset>0.30</offset>
          </li>
        </capMods>
		<statOffsets>
			<AimingDelayFactor>-0.15</AimingDelayFactor>
			<PainShockThreshold>.28</PainShockThreshold>
			<ArmorRating_Blunt>.35</ArmorRating_Blunt>
			  <ArmorRating_Sharp>.35</ArmorRating_Sharp>
			  <ArmorRating_Heat>.35</ArmorRating_Heat>
			  <ArmorRating_Alignment>.35</ArmorRating_Alignment>
		</statOffsets>
      </li>
	  <li>
        <label>IV</label>
        <minSeverity>3</minSeverity>
		<becomeVisible>true</becomeVisible>
		<capMods>
		  <li>
            <capacity>Moving</capacity>
            <offset>0.60</offset>
		  </li>
	      <li>
            <capacity>Manipulation</capacity>
            <offset>0.60</offset>
          </li>
		  <li>
            <capacity>Breathing</capacity>
            <offset>0.35</offset>
          </li>
		  <li>
            <capacity>BloodPumping</capacity>
            <offset>0.35</offset>
          </li>
		  <li>
            <capacity>BloodFiltration</capacity>
            <offset>0.35</offset>
          </li>
        </capMods>
		<statOffsets>
			<AimingDelayFactor>-0.15</AimingDelayFactor>
