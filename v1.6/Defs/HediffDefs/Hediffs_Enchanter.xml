  <HediffDef >
    <defName>TM_EnchantedBodyHD</defName>
	<hediffClass>HediffWithComps</hediffClass>
	<label>Enchanted Body</label>
	<description>Creation of enchanted body parts to enhance physical capability.</description>
    <defaultLabelColor>(0.6,0.8,1)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
	<makesSickThought>false</makesSickThought>
	<tendable>false</tendable>
    <makesAlert>false</makesAlert>
	<minSeverity>0.001</minSeverity>
	<maxSeverity>4.0</maxSeverity>
	<isBad>false</isBad>
	<initialSeverity>.5</initialSeverity>
	<stages>
      <li>
        <label>Arm</label>
        <minSeverity>0.001</minSeverity>
		<becomeVisible>true</becomeVisible>
		<restFallFactor>1.2</restFallFactor>
		<hungerRateFactor>1.2</hungerRateFactor>
		<capMods>
		  <li>
			  <capacity>Manipulation</capacity>
			  <offset>0.25</offset>
		  </li>
		</capMods>
		<statOffsets>
		  <ImmunityGainSpeed>0.10</ImmunityGainSpeed>
		  <PainShockThreshold>0.05</PainShockThreshold>
		  <AimingDelayFactor>-0.05</AimingDelayFactor>
	    </statOffsets>
      </li>
	  <li>
        <label>Two Arms</label>
        <minSeverity>1.0</minSeverity>
		<becomeVisible>true</becomeVisible>
		<restFallFactor>1.3</restFallFactor>
		<hungerRateFactor>1.3</hungerRateFactor>
		<capMods>
		  <li>
			  <capacity>Manipulation</capacity>
			  <offset>0.35</offset>
		  </li>
		</capMods>
		<statOffsets>
		  <ImmunityGainSpeed>0.15</ImmunityGainSpeed>
		  <PainShockThreshold>0.08</PainShockThreshold>
		  <AimingDelayFactor>-0.08</AimingDelayFactor>
	    </statOffsets>
      </li>	 
	  <li>
        <label>Arms and Eye</label>
        <minSeverity>2.0</minSeverity>
		<becomeVisible>true</becomeVisible>
		<restFallFactor>1.35</restFallFactor>
		<hungerRateFactor>1.3</hungerRateFactor>
		<capMods>
		  <li>
			  <capacity>Manipulation</capacity>
			  <offset>0.35</offset>
		  </li>
		  <li>
            <capacity>Sight</capacity>
            <offset>0.25</offset>
          </li>
		</capMods>
		<statOffsets>
		  <ImmunityGainSpeed>0.15</ImmunityGainSpeed>
		  <PainShockThreshold>0.09</PainShockThreshold>
		  <AimingDelayFactor>-0.09</AimingDelayFactor>
