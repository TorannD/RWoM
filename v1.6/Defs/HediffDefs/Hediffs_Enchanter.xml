  <HediffDef >
    <defName>TM_EnchantedAuraHD</defName>
	<hediffClass>HediffWithComps</hediffClass>
	<label>Enchanted Aura</label>
	<description>An enchanted aspect surrounds the pawn enhancing qualities considered for beauty, endearment, and attraction.</description>
    <defaultLabelColor>(0.6,0.8,1)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
	<makesSickThought>false</makesSickThought>
	<tendable>false</tendable>
    <makesAlert>false</makesAlert>
	<minSeverity>0.001</minSeverity>
	<maxSeverity>4.0</maxSeverity>
	<isBad>false</isBad>
	<initialSeverity>.5</initialSeverity>
	<stages>
      <li>
        <label>I</label>
        <minSeverity>0.001</minSeverity>
		<becomeVisible>true</becomeVisible>
		<socialFightChanceFactor>-1</socialFightChanceFactor>
		<capMods>
		  <li>
            <capacity>Hearing</capacity>
            <offset>0.2</offset>
          </li>
		  <li>
            <capacity>Talking</capacity>
            <offset>0.25</offset>
          </li>
        </capMods>
		<statOffsets>
			<MentalBreakThreshold>-.10</MentalBreakThreshold>
			<SocialImpact>0.30</SocialImpact>
			<TradePriceImprovement>0.05</TradePriceImprovement>
			<NegotiationAbility>0.25</NegotiationAbility>
		</statOffsets>
      </li>
	  <li>
        <label>II</label>
        <minSeverity>1</minSeverity>
		<becomeVisible>true</becomeVisible>
		<socialFightChanceFactor>-1</socialFightChanceFactor>
		<capMods>
		  <li>
            <capacity>Hearing</capacity>
            <offset>0.25</offset>
          </li>
		  <li>
            <capacity>Talking</capacity>
            <offset>0.3</offset>
          </li>
        </capMods>
		<statOffsets>
			<MentalBreakThreshold>-.12</MentalBreakThreshold>
			<SocialImpact>0.35</SocialImpact>
			<TradePriceImprovement>0.075</TradePriceImprovement>
			<NegotiationAbility>0.30</NegotiationAbility>
		</statOffsets>
      </li>
	  <li>
        <label>III</label>
        <minSeverity>2</minSeverity>
		<becomeVisible>true</becomeVisible>
		<socialFightChanceFactor>-1</socialFightChanceFactor>
		<capMods>
		  <li>
            <capacity>Hearing</capacity>
            <offset>0.3</offset>
          </li>
		  <li>
            <capacity>Talking</capacity>
            <offset>0.35</offset>
          </li>
        </capMods>
		<statOffsets>
			<MentalBreakThreshold>-.14</MentalBreakThreshold>
			<SocialImpact>0.40</SocialImpact>
			<TradePriceImprovement>0.1</TradePriceImprovement>
			<NegotiationAbility>0.35</NegotiationAbility>
		</statOffsets>
      </li>
	  <li>
        <label>IV</label>
        <minSeverity>3</minSeverity>
		<becomeVisible>true</becomeVisible>
		<socialFightChanceFactor>-1</socialFightChanceFactor>
		<capMods>
		  <li>
            <capacity>Hearing</capacity>
            <offset>0.35</offset>
          </li>
		  <li>
            <capacity>Talking</capacity>
            <offset>0.4</offset>
          </li>
        </capMods>
		<statOffsets>
			<MentalBreakThreshold>-.15</MentalBreakThreshold>
			<SocialImpact>0.45</SocialImpact>
			<TradePriceImprovement>0.125</TradePriceImprovement>
			<NegotiationAbility>0.40</NegotiationAbility>
		</statOffsets>
      </li>	  
	</stages>
  </HediffDef>
  
  <HediffDef >
    <defName>TM_ShapeshiftHD</defName>
	<hediffClass>HediffWithComps</hediffClass>
	<label>Shapeshifter</label>
	<description>Improves the power of any animal form taken by a shape shifter.</description>
