<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <!-- TMagic quests-->

  <Operation Class="PatchOperationSequence">
	<success>Always</success>
	<operations>
		<li Class="PatchOperationTest">
			<xpath>*/RimQuest.QuestGiverDef[defName = "RQ_MedievalQuestGiver" or defName = "RQ_TribalQuestGiver"]/quests</xpath>
		</li>
	   <li Class="PatchOperationAdd">
			<xpath>*/RimQuest.QuestGiverDef[defName = "RQ_MedievalQuestGiver" or defName = "RQ_TribalQuestGiver"]/quests</xpath>
			<value>
				<QuestArcaneCamp>3</QuestArcaneCamp>
			</value>
	  </li>
	 </operations>
  </Operation>	  
  
</Patch>
