<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--================== Material Resource Recipes ==================-->
  
  <RecipeDef>
    <defName>TM_MakeManaweave</defName>
    <label>manaweave</label>
    <description>Weave mana, magicyte, and cloth into manaweave</description>
    <jobString>weaving mana</jobString>
<!-- 	<researchPrerequisite>TechPlasteelCrafting</researchPrerequisite> -->
    <workSpeedStat>TailoringSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>6000</workAmount>
	<workSkillLearnFactor>1.1</workSkillLearnFactor>
<!-- 	<recipeUsers>
		<li>TableGemcutting</li>
	</recipeUsers> -->
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawMagicyte</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Cloth</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <products>
      <TM_Manaweave>25</TM_Manaweave>
    </products>
    <skillRequirements>
	  <Intellectual>4</Intellectual>
    </skillRequirements>
    <workSkill>Intellectual</workSkill>
  </RecipeDef>
  
  <RecipeDef>
    <defName>TM_MakeArcalleum</defName>
    <label>arcalleum</label>
    <description>Meld steel, jade, silver and magicyte into arcalleum alloy</description>
    <jobString>smelting arcalleum</jobString>
<!-- 	<researchPrerequisite>TechPlasteelCrafting</researchPrerequisite> -->
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workAmount>6000</workAmount>
	<workSkillLearnFactor>1.1</workSkillLearnFactor>
<!-- 	<recipeUsers>
		<li>TableGemcutting</li>
	</recipeUsers> -->
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawMagicyte</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Jade</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <products>
      <TM_Arcalleum>50</TM_Arcalleum>
    </products>
    <skillRequirements>
	  <Intellectual>4</Intellectual>
    </skillRequirements>
    <workSkill>Intellectual</workSkill>
  </RecipeDef>
  
  
</Defs>
