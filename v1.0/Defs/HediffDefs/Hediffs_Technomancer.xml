  <HediffDef>
    <defName>TM_NanoStimulantWithdrawalHD</defName>
	<hediffClass>HediffWithComps</hediffClass>
	<label>stim withdrawal</label>
    <defaultLabelColor>(0.8,0,0)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
	<makesSickThought>false</makesSickThought>
	<tendable>false</tendable>
    <makesAlert>false</makesAlert>
	<maxSeverity>1</maxSeverity>
	<initialSeverity>0.95</initialSeverity>
	<isBad>true</isBad>
	<comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-15</severityPerDay>
      </li>	  
    </comps>
	<stages>
      <li>
        <label></label>
        <minSeverity>0.001</minSeverity>
		<restFallFactor>1.2</restFallFactor>
		<painOffset>+0.3</painOffset>   
		<capMods>
		  <li>
            <capacity>Moving</capacity>
            <setMax>.5</setMax>
          </li>
		  <li>
            <capacity>Manipulation</capacity>
            <offset>-0.2</offset>
          </li>
		  <li>
            <capacity>Consciousness</capacity>
            <offset>-0.2</offset>
          </li>
		  <li>
			<capacity>Sight</capacity>
			<offset>-0.30</offset>
		  </li>
		</capMods>
		<statOffsets>
		  <PainShockThreshold>-0.15</PainShockThreshold>
		  <MeleeDodgeChance>-15</MeleeDodgeChance>
		  <AimingDelayFactor>+0.5</AimingDelayFactor>
	    </statOffsets>
      </li>      
	</stages>
  </HediffDef>
  
